{
  "name": "foglet-core",
  "version": "3.1.1",
  "description": "Core of the foglet library",
  "main": "./foglet-core.js",
  "scripts": {
    "lint": "eslint src/",
    "esdoc": "esdoc && npm run esdoc:example",
    "esdoc:example": "cp -R ./dist ./example ./docs",
    "all": "npm run build:foglet && npm run build:foglet-min && npm test && npm run esdoc",
    "test": "foglet-scripts test",
    "server": "foglet-scripts start",
    "build:foglet": "mkdir -p dist/ && browserify -r ./foglet-core.js:foglet -t [ babelify --presets [ env ] ]  > ./dist/foglet.bundle.js",
    "build:watch": "mkdir -p dist/ && watchify -r ./foglet-core.js:foglet -t [ babelify --presets [ env ] ]  -o ./dist/foglet.bundle.js -v -d ",
    "build:foglet-min": "mkdir -p dist/ && browserify -r ./foglet-core.js:foglet -t [ babelify --presets [ env ] ] | uglifyjs -c > ./dist/foglet.bundle.min.js",
    "wbuild:foglet": "browserify -r ./foglet-core.js:foglet -t [ babelify --presets [ env ] ]  > ./dist/foglet.bundle.js",
    "wbuild:watch": "watchify -r ./foglet-core.js:foglet -t [ babelify --presets [ env ] ]  -o ./dist/foglet.bundle.js -v -d ",
    "wbuild:foglet-min": "browserify -r ./foglet-core.js:foglet -t [ babelify --presets [ env ] ] | uglifyjs -c > ./dist/foglet.bundle.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ran3d/foglet-core.git"
  },
  "keywords": [
    "foglet",
    "spray-wrtc",
    "browser-to-browser",
    "webRTC",
    "foglet computing"
  ],
  "author": "Folkvir",
  "contributors": [
    "Chat Wane (https://github.com/Chat-Wane)",
    "Thomas Minier (https://github.com/Callidon)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ran3d/foglet-core/issues"
  },
  "foglet-scripts": {
    "browsers": [
      "Firefox", "Firefox", "Firefox", "Firefox", "Firefox", "Firefox", "Firefox"
    ],
    "timeout": 20000,
    "lint": false
  },
  "homepage": "https://github.com/ran3d/foglet-core#readme",
  "dependencies": {
    "debug": "^2.6.8",
    "lodash.merge": "^4.6.0",
    "lodash.sortedindexby": "^4.6.0",
    "readable-stream": "^2.3.3",
    "socket.io-client": "^1.7.4",
    "spray-wrtc": "^2.0.2",
    "tman-wrtc": "0.0.8",
    "unicast-definition": "^2.0.0",
    "uuid": "^3.0.1",
    "version-vector-with-exceptions": "^0.1.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.1",
    "eslint": "^3.13.1",
    "foglet-scripts": "git+https://github.com/ran3d/foglet-scripts.git",
    "karma-firefox-launcher": "^1.0.0",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "watchify": "^3.7.0"
  },
  "bundledDependencies": [
    "debug",
    "lodash.merge",
    "lodash.sortedindexby",
    "readable-stream",
    "socket.io-client",
    "spray-wrtc",
    "unicast-definition",
    "uuid",
    "version-vector-with-exceptions"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  }
}
